{
  "name": "ToriiJusetsu",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "lint": "astro sync && eslint . --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:astro": "prettier --write '**/*.astro'",
    "dev:format": "prettier --write . && npm run dev",
    "save": "prettier --write . && echo 'All files formatted!'",
    "watch": "prettier --write . && astro dev --watch",
    "deploy": "npm run build && npx wrangler deploy"
  },
  "dependencies": {
    "@astrojs/react": "^4.2.4",
    "@fontsource-variable/inter": "^5.2.6",
    "@fontsource-variable/inter-tight": "^5.2.6",
    "@fontsource-variable/noto-sans-jp": "^5.2.6",
    "@iconify-json/fa-brands": "^1.2.1",
    "@iconify-json/lucide": "^1.2.57",
    "@tailwindcss/vite": "^4.1.4",
    "astro": "^5.7.2",
    "astro-icon": "^1.1.5",
    "clsx": "^2.1.1",
    "framer-motion": "^12.7.4",
    "gsap": "^3.12.7",
    "keen-slider": "^6.8.6",
    "lenis": "^1.2.3",
    "lucide-react": "^0.488.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-fast-marquee": "^1.6.5",
    "sharp": "^0.34.3",
    "swiper": "^11.2.6",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@eslint/js": "^9.35.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.30.1",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-better-tailwindcss": "^3.7.7",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0"
  },
  "packageManager": "npm@11.4.2"
}
